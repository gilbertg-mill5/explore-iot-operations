apiVersion: v1
kind: Service
metadata:
  name: mqtt-service
  annotations:
    service.beta.kubernetes.io/azure-load-balancer-internal: "true"
    service.beta.kubernetes.io/azure-load-balancer-resource-group: "training"
    service.beta.kubernetes.io/azure-dns-label-name: "mill5iotgilbert"
    service.beta.kubernetes.io/azure-pls-create: "true"
  labels:
    app: aio-mq-operator
    app: health-manager
    app.kubernetes.io/instance: aio-mq-dmqtt-health-manager
    app.kubernetes.io/managed-by: aio-mq-operator
    app.kubernetes.io/name: dmqtt
spec:
  type: LoadBalancer
  ports:
    - name: mqtt
      protocol: TCP
      port: 1883
      targetPort: 1883
    - name: mqtts
      protocol: TCP
      port: 8883
      targetPort: 8883
  selector:
    app: mqtt-service
